{# empty Twig template #}
{% extends "VostreCentralBundle::layout.html.twig" %}       
{% block body %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-6">
                <h4>Dados - {{ acessosDados|length }} acesso(s)</h4>
                {% for acesso in acessosDados %}
                    <p>{{ acesso.dataCriacao|date('d/m/Y H:i:s') }}</p>
                {% endfor %}
            </div>
            <div class="col-md-6">
                <h4>Mensagens - {{ acessosMensagens|length }} acesso(s)</h4>
                {% for acesso in acessosMensagens %}
                    <p>{{ acesso.dataCriacao|date('d/m/Y H:i:s') }}</p>
                {% endfor %}
            </div>
        </div>
    </div>
{% endblock %}
