<div class="container-fluid">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <ul>
                {% for horarioItinerario in horarios %}
                    <li>{{ horarioItinerario.horario.nome|date('H:i') }}</li>
                {% endfor %}
            </ul>
            <ul>
                {% for alt in alternativas %}
                    <li>{{ alt.itinerario }}</li>
                {% endfor %}
            </ul>
        </div>
    </div>
</div>