# Do not remove this line or mod_rewrite rules and search engine friendly URLs will stop working
RewriteBase /






# DO NOT REMOVE THIS LINE AND THE LINES BELOW ERRORPAGEID:yWuzub
ErrorDocument 404 /404.html
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE yWuzub:ERRORPAGEID


RewriteEngine on
RewriteCond %{HTTP_HOST} ^vostre.dev.br$ [NC,OR]
RewriteCond %{HTTP_HOST} ^www.vostre.dev.br$
RewriteCond %{REQUEST_URI} !web/
RewriteRule (.*) /web/$1 [L]


# DO NOT REMOVE THIS LINE AND THE LINES BELOW ERRORPAGEID:aJunem
ErrorDocument 500 /500.html
# DO NOT REMOVE THIS LINE AND THE LINES ABOVE aJunem:ERRORPAGEID

